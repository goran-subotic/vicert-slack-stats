<p id="notice"><%= notice %></p>

<h3>Listing Job Scores</h3>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Job</th>
      <th>Start time</th>
      <th>End time</th>
      <th colspan="3">Action</th>
    </tr>
  </thead>

  <tbody>
    <% @job_scores.each do |job_score| %>
      <tr>
        <td><%= job_score.name %></td>
        <td><%= job_score.start_time %></td>
        <td><%= job_score.end_time %></td>
        <td><%= link_to 'Show', job_score %></td>
        <td><%= link_to 'Edit', edit_job_score_path(job_score) %></td>
        <td><%= link_to 'Destroy', job_score, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
    <tr>
	   <td colspan="6"><%= will_paginate @job_scores %></td>
	</tr>
  </tbody>
</table>

<br>

<%= link_to 'New Job score', new_job_score_path %>
